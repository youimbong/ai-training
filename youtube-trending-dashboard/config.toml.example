# YouTube Trending Dashboard Configuration
# Copy this file to config.toml and update the values

[app]
title = "YouTube 인기 동영상 대시보드"
icon = "📺"
version = "1.0.0"
debug = false

[youtube_api]
key = "your_youtube_api_key_here"
default_region = "KR"
default_category = 0  # 0 = All categories
max_results = 30

[cache]
ttl = 300  # 5 minutes in seconds
enabled = true

[logging]
level = "INFO"
file = "logs/app.log"

[ui]
default_theme = "light"
enable_dark_mode = true
items_per_page = 30

[performance]
max_retries = 3
request_timeout = 30
enable_lazy_loading = true

[security]
enable_rate_limiting = true
max_requests_per_minute = 100
